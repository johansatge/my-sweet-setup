"*":
  "atom-beautify":
    general:
      _analyticsUserId: "6325ec04-0111-44b9-b65d-9321c5979005"
    php:
      cs_fixer_path: "/usr/local/bin/php-cs-fixer"
  "atom-package-deps":
    ignored: [
      "linter-ui-default"
    ]
  "autocomplete-php":
    executablePath: '''
      /usr/local/opt/php56/bin/php

    '''
  core:
    allowPendingPaneItems: false
    disabledPackages: [
      "metrics"
      "about"
      "archive-view"
      "autocomplete-snippets"
      "autosave"
      "background-tips"
      "bookmarks"
      "exception-reporting"
      "language-c"
      "language-clojure"
      "language-csharp"
      "language-java"
      "language-less"
      "language-objective-c"
      "language-perl"
      "language-python"
      "language-ruby"
      "language-ruby-on-rails"
      "package-generator"
      "snippets"
      "welcome"
      "autoflow"
      "github"
    ]
    openEmptyEditorOnStart: false
    telemetryConsent: "no"
    themes: [
      "native-ui"
      "monokai"
    ]
  editor:
    fontFamily: "Menlo"
    fontSize: 12
    lineHeight: 1.5
    preferredLineLength: 120
    showInvisibles: true
  "exception-reporting":
    userId: "099fb1ee-8ae9-9711-b585-86b591b68543"
  "git-diff":
    showIconsInEditorGutter: true
  linter: {}
  "linter-eslint":
    autofix:
      fixOnSave: true
  "linter-ui-default": {}
  "markdown-preview":
    useGitHubStyle: true
  "prettier-atom":
    formatOnSaveOptions:
      isDisabledIfNoConfigFile: true
      isDisabledIfNotInPackageJson: true
      showInStatusBar: true
  tabs: {}
  "tree-view": {}
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".ini.source":
  editor:
    tabLength: 4
